{
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png)",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }]
      }, {
        "source" : "**/*.html",
        "headers" : [{
          "key" : "Content-Security-Policy",
          "value" : "script-src 'self' 'unsafe-inline';"
        }]
      }
    ]
  }